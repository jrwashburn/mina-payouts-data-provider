{
  "name": "mina-payouts-data-provider",
  "version": "2.6.9",
  "description": "Data provider API for mina-pool-payouts",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --outDir dist",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node dist/index.js\" | pino-pretty",
    "dev-sl": "nodemon --watch src --ext ts --exec \"npm run build && node dist/index.js\" | pino-pretty",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "dotenv -e tests/.env.test -- node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit",
    "test:watch": "dotenv -e tests/.env.test -- node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "dotenv -e tests/.env.test -- node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --forceExit",
    "test:integration": "dotenv -e tests/.env.test.integration -- node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit",
    "test:integration:watch": "dotenv -e tests/.env.test.integration -- node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:db:start": "bash tests/start-test-databases.sh",
    "test:db:stop": "bash -c 'if command -v docker-compose &> /dev/null; then docker-compose -f docker-compose.test.yml down; else docker compose -f docker-compose.test.yml down; fi'",
    "test:db:reload": "bash tests/reload-test-databases.sh",
    "test:db:refresh": "bash -c 'if command -v docker-compose &> /dev/null; then docker-compose -f docker-compose.test.yml down -v; else docker compose -f docker-compose.test.yml down -v; fi' && bash tests/start-test-databases.sh",
    "test:fixtures": "node dist/tests/utils/fixtureCapture.js"
  },
  "keywords": [],
  "author": "jrwashburn,pabloabadcarmona",
  "license": "Apache-2.0",
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "cors": "^2.8.5",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.5.0",
    "express": "5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.5.0",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.0",
    "node-cron": "^4.0.7",
    "pg": "^8.15.6",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "response-time": "^2.3.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.14",
    "@types/response-time": "^2.3.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.32.1"
  }
}
